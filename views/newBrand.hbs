<h1>ADD A NEW BRAND HERE</h1>
{{#if message}}
    <h2>{{message}}</h2>
{{/if}}
<div>
    <form id="newBrandPoster" method="POST" action="/addnew">
        Brand name:<input type="text" name="BrandName">
        <br>
        Brand Location:<input type="text" name="BrandLocation">
        <br>
        Description:
        <textarea name="descp" id="input" cols="50" rows="10"></textarea>
        <br>
        <input type="Submit" value="Submit">
    </form>
</div>